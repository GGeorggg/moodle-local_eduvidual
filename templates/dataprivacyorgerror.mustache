<div class="local_eduvidual dataprivacyorgerror">
    <div class="alert alert-danger">
        {{#str}} dataprivacyorgerror:description, local_eduvidual {{/str}}
    </div>

    <table class="generaltable">
        <thead>
            <tr>
                <td>
                    {{#str}} dataprivacyorgerror:orgid, local_eduvidual {{/str}}
                </td>
                <td>
                    {{#str}} dataprivacyorgerror:orgname, local_eduvidual {{/str}}
                </td>
                <td>
                    {{#str}} dataprivacyorgerror:contacts, local_eduvidual {{/str}}
                </td>
            </tr>
        </thead>
        <tbody>
            {{#orgs}}
            <tr>
                <td>
                    <a href="{{wwwroot}}/course/index.php?categoryid={{categoryid}}">
                        {{orgid}}
                    </a>
                </td>
                <td>
                    <a href="{{wwwroot}}/course/index.php?categoryid={{categoryid}}">
                        {{name}}
                    </a>
                </td>
                <td>
                    {{#managers}}
                        <div>
                            <a href="/user/profile.php?id={{id}}" target="_blank">
                                {{{userpicture}}}
                                {{userfullname}}
                            </a>
                        </div>
                    {{/managers}}
                </td>
            </tr>
            {{/orgs}}
        </tbody>
    </table>
</div>
