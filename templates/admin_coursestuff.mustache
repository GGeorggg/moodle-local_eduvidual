<h4>{{#str}} admin:coursebasements:title, block_eduvidual {{/str}}</h4>
<p>{{#str}} admin:coursebasements:description, block_eduvidual {{/str}}</p>
<input id="block_eduvidual_admin_coursebasements" value="{{coursebasements}}" onkeyup="require(['block_eduvidual/admin'], function(ADMIN) { ADMIN.coursebasements(); });" />

<h4>{{#str}} admin:orgcoursebasement:title, block_eduvidual {{/str}}</h4>
<p>{{#str}} admin:orgcoursebasement:description, block_eduvidual {{/str}}</p>
<select id="block_eduvidual_admin_orgcoursebasement" onchange="var sel = this; require(['block_eduvidual/admin'], function(ADMIN) { ADMIN.orgcoursebasement(sel.value); });">
{{#orgcoursebasements}}
    <optgroup label="{{key}}">
        {{#basements}}
            <option value="{{id}}"{{#selected}} selected{{/selected}}>{{fullname}}</option>
        {{/basements}}
    </optgroup>
{{/orgcoursebasements}}
{{^orgcoursebasements}}
    <p class="alert alert-info">{{#str}} orgcoursebasement:nocoursebasementsgiven, block_eduvidual {{/str}}</p>
{{/orgcoursebasements}}
</select>

